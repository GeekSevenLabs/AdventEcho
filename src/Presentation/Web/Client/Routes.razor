@inject NavigationManager NavigationManager

<Router AppAssembly="typeof(Program).Assembly">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(AuthenticatedLayout)">
            <NotAuthorized>
                <EchoRedirectToLogin />
            </NotAuthorized>
        </AuthorizeRouteView>
    </Found>
</Router>